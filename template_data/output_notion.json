  "views": [
    {
      "view_name": "All Assignments",
      "type": "table",
      "filter": {},
      "sort": [
        {
          "property": "Due Date",
          "direction": "ascending"
        }
      ]
    },
    {
      "view_name": "This Week",
      "type": "table",
      "filter": {
        "and": [
          {
            "property": "Due Date",
            "date": {
              "next_week": {}
            }
          },
          {
            "property": "Status",
            "select": {
              "does_not_equal": "submitted"
            }
          }
        ]
      },
      "sort": [
        {
          "property": "Priority",
          "direction": "descending"
        }
      ]
    },
    {
      "view_name": "By Class",
      "type": "board",
      "group_by": "Class",
      "filter": {
        "property": "Status",
        "select": {
          "does_not_equal": "submitted"
        }
      }
    },
    {
      "view_name": "Calendar",
      "type": "calendar",
      "calendar_by": "Due Date"
    }
